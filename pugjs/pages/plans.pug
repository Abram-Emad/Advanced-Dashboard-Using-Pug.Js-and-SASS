//- Start Page
extends index

//- Change Page Title
block pageTitle    
    title #{arrayOfTitles[7]}

//- Change Page Content
block contentTitle
    h1.p-relative.mt-20.mr-20.mb-40.ml-40 #{arrayOfTitles[7]}

block pageContent
    .plans-page.d-grid.m-20.gap-20 
        +plansPageContent("Free", "0.00", "", "", ".green", "bg-green")        
        +plansPageContent("Basic", "7.99", "fyesorno", "", ".blue", "bg-blue")        
        +plansPageContent("Premium", "19.99", "fyesorno", "syesorno", ".orange", "bg-orange")        
//- End Page